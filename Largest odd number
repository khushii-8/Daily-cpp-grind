b#include <iostream>
#include <string>
using namespace std;

string largestOddNumber(string num) {
    // Start from the last digit and move left
    for (int i = num.size() - 1; i >= 0; i--) {
        char ch = num[i];
        // Check if this digit is odd
        if (ch == '1' || ch == '3' || ch == '5' || ch == '7' || ch == '9') {
            // Return substring from 0 to i (inclusive)
            return num.substr(0, i + 1);
        }
    }
    // No odd digit found
    return "";
}

int main() {
    cout << largestOddNumber("35427") << endl; // 35427
    cout << largestOddNumber("4206") << endl;  // ""
    cout << largestOddNumber("52") << endl;    // 5
    return 0;
}
