#include <iostream>
#include <vector>
using namespace std;

int missingNumber(vector<int>& nums) {
    int n = nums.size();
    int expectedSum = n * (n + 1) / 2;
    int actualSum = 0;

    for (int num : nums)
        actualSum += num;

    return expectedSum - actualSum;
}

int main() {
    vector<int> arr = {3, 0, 1};
    cout << "Missing number: " << missingNumber(arr) << endl;
    return 0;
}
