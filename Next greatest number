class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        int n=letters.size();
        int s=0;
        char ans=letters[0];
        int e=n-1;
        while(s<=e){
            int m=(s+e)/2;
            if(letters[m]>target){
                ans=letters[m];
                e=m-1;
            }
            else{
                s=m+1;
            }            
        }
        return ans;
    }
};
