#include <bits/stdc++.h>
using namespace std;

bool subsetSum(int arr[], int n, int k) {
    // Base Cases
    if (k == 0) return true;     // found subset
    if (n == 0) return false;    // no elements left

    if (arr[n-1] <= k) {
        // include OR exclude current element
        return subsetSum(arr, n-1, k-arr[n-1]) || subsetSum(arr, n-1, k);
    } else {
        // skip current element
        return subsetSum(arr, n-1, k);
    }
}

int main() {
    int arr[] = {3, 34, 4, 12, 5, 2};
    int n = 6, k = 9;

    if (subsetSum(arr, n, k)) cout << "YES";
    else cout << "NO";

    return 0;
}
