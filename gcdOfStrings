class Solution {
public:
    string gcdOfStrings(string str1, string str2) {
        
        // Check if pattern matches
        if (str1 + str2 != str2 + str1)
            return "";
        
        // Find gcd of lengths
        int n = str1.length();
        int m = str2.length();
        int gcdLength = __gcd(n, m);
        
        return str1.substr(0, gcdLength);
    }
};
