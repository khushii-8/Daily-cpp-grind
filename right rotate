class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n=nums.size();
        k=k%n;
        reverse(nums.begin(),nums.end());  
        reverse(nums.begin(),nums.begin()+k);
        reverse(nums.begin()+k,nums.end());
             
    }
};
//if we place the first reverse to the end then it will become the rotation to left side
